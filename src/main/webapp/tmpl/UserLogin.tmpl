<script id="tmpl-UserLogin" type="text/html">

	<div class="UserLogin">
		<div class="container">
			<div class="login">
				<h2>Login</h2>
				<div class="field">
					<label>UserName:</label>
					<input data-dojo-type="dijit/form/TextBox" type="text" />
				</div>
				
				<div class="field">
					<label>PassWord:</label>
					<input data-dojo-type="dijit/form/TextBox" type="password"/>
				</div>
				
				<div class="field">
					<button id="login" data-dojo-type="dijit/form/Button">
						Login
					</button>
				</div>
			</div>
			
		
			<div class="register">
				<h2>Register</h2>
				<div class="field">
					<label>UserName:</label>
					<input data-dojo-type="dijit/form/TextBox" id="register-username" type="text"  />
				</div>
				
				<div class="field">
					<label>PassWord:</label>
					<input data-dojo-type="dijit/form/TextBox" id="register-password" type="password" />
				</div>
				
				<div class="field">
					<label>ReType PassWord:</label>
					<input data-dojo-type="dijit/form/TextBox" id="register-repassword" type="password" />
				</div>
				
				<div class="field">
					<button id="register" data-dojo-type="dijit/form/Button">
						Register
					</button>
				</div>
			</div>
			
			</div>
			
	</div>
	
</script>